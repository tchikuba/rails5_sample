<h1>Log in</h1>
<div>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>
      <div class="field">
        <%= f.label :email %>
        <%= f.email_field :email %>
      </div>
      <div class="field">
        <%= f.label :password %>
        <%= f.password_field :password %>
      </div>
      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
    <% end %>
    <p>New user? <%= link_to "Sign up now!", new_user_path %></p>
  </div>
</div>
